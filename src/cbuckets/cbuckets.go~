package cbuckets

import (
	"howler"
	"time"
)



// uses a skiplist to maintain a sorted queue
type buckets struct {
	c    (<-chan howler.Callback)
}

func NewBuckets() *buckets {
	q := new(buckets)
	return q
}

func (q *buckets) Add(c howler.Callback, after time.Duration) (string, error) {
	return "", nil
}

func (q *buckets) Del(id string) error {
	return nil
}

func (q *buckets) Howls() <-chan howler.Callback {
	return q.c
}
